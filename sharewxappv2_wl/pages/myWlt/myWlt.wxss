page {
  height: 98%;
  background-color:#F5F5F5;
}
.itemCls{
  width: 90%;
  margin-left: 15px;
  margin-right: 0px;
  margin-top: 20px;
  margin-bottom: 10px;
  border-radius: 10px;
  box-shadow:1px 3px 3px 1px rgba(0,0,0,0.3);
  background:rgba(255,255,255,1);
}
.itemTitleCls{
  width:95%;
  padding-top: 15px;
  height:140px;
}
.itemBoxCls {
  height: 110px;
  margin-left: 15px;
  display: flex;
  align-items: center;
  justify-content:right;
}
.itemBoxLeftCls{
  float:left;
  color: #000;
  margin-left: 10px;
  width: 120px;
  text-align: center;
  font-size:15px;
  font-family:PingFangSC-Regular;
  font-weight:400;
}
.itemBoxLeftCls .up {
  margin-right: 2rpx;
  font-size: 20px;
  text-align: center;
  margin-top: 2rpx;
  width: 120px;
  margin-bottom: 10rpx;
}
.itemBoxLeftCls .down {
  margin-right: 2rpx;
  font-size: 15px;
  text-align: center;
  margin-top: 2rpx;
  width: 120px;
  margin-bottom: 10rpx;
}

.itemBosRightCls{
  text-align:right;
  width: 50%;
  margin-left: 15px;
}
.itemBosRightCls button{
  background-color: #FF610A
}
.listitem{
  width: 100%;
  height: 50px;
  line-height: 40px;
  text-align: center;
  margin-top: 20px;
  padding-top: 10px;
  padding-left: 15px;
  background-color:#f5f5ff;
}
.listItemLeft{
  float: left;
  margin-left: 10px;
}
.listItemRight{
  text-align: right;
  margin-right: 40px;
}