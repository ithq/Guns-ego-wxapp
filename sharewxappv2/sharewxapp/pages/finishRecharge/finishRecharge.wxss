}
.layergotolook{
  position: relative;
  width: 80.63%;
  height: 95.9%;
  margin: 0 auto;
  text-align: center;
}
.looktopbg{
  position: absolute;
  top: 0;
  width: 100%;
  height: 20.23%;
  margin: 0 auto;
}
.lookbottbg{
  position: absolute;
  top: 20.23%;
  width: 100%;
  height: 79.77%;
  border-radius: 0 0 50rpx 50rpx;
  background: #ff1a3d;
  /*box-shadow: 0 -3px 1px 2px rgba(173,15,4,.35) inset;*/
}
.cardbg{
  position: relative;
  width: 90.78%;
  height: 54.50%;
  margin: 0 auto;
  border-radius: 20rpx;
  background: -webkit-linear-gradient(top,#fff3d1,#fae7b0);
}
.cardbg:before{
  content: '';
  position: absolute;
  top: 20.5%;
  left: -11rpx;
  width: 22rpx;
  height: 22rpx;
  background: #ff1a3d;
  border-radius: 50%;
}
.cardbg:after{
  content: '';
  position: absolute;
  top: 20.5%;
  right: -11rpx;
  width: 22rpx;
  height: 22rpx;
  background: #ff1a3d;
  border-radius: 50%;
}
.cardbgblbg{
    position: absolute;
    bottom: -10rpx;
    left: -10rpx;
    width: 0;
    height: 0;
    border-bottom: 90rpx solid #ff1a3d;
    border-right: 90rpx solid transparent;
}
.cardbgbrbg{
  position: absolute;
  bottom: -10rpx;
  right: -10rpx;
  width: 0;
  height: 0;
  border-bottom: 100rpx solid #ff1a3d;
  border-left: 100rpx solid transparent;
  transform: rotate(10deg);
  -webkit-transform: rotate(10deg);
}
