}
/*民生银行广告*/
.layer{
    z-index: 11;
    display: flex;
    justify-content: center;
    align-items: center;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.6);
}
.layercontbox{
    position: relative;
    width: 85.3%;
    height: 73.13%;
}
.dongman{
  z-index: 2;
  position: absolute;
  top: 0;
  width: 100%;
  height: 36.25%;
}
.layercont{
    position: relative;
    width: 80.63%;
    height: 75%;
    margin: 0 auto;
    border-radius: 50rpx;
    text-align: center;
    background: #df0132;
    box-shadow: 0 0 5px rgba(173,15,4,.35) inset;
}

.jinbi{
  z-index: 1;
  position: absolute;
  width: 100%;
  height: 40.75%;
  border-radius: 40rpx 40rpx 0 0;
}
.jinbi image{
  display: block;
  width: 100%;
  height: 96%;
  border-radius: 40rpx 40rpx 0 0;
}
.txing{
    position: absolute;
    top: 0;
    left: 0;
    width:100%;
    height:100%;
    text-align:center;
    color:#fff;
    background-color:#ff1a3d;
    border-radius: 40rpx;
    overflow: hidden;
}
.txing:before{
  content: '';
  display: block;
  position: absolute;
  left: -27%;
  top: 0%;
  width: 50%;
  height: 37.87%;
  transform: skewX(-45deg);
  background: #df0132;
  box-shadow: 0 0 5px rgba(173,15,4,.35) inset;
}
.txing:after{
    content:'';
    display:block;
    position:absolute;
    top:0%;
    right:-27%;
    width:50%;
    height:37.87%;
    transform:skewX(45deg);
    background:#df0132;
    box-shadow: 0 0 5px rgba(173,15,4,.35) inset;
}
.gongxi,.getreward{
  position: relative;
  z-index: 3;
  text-align: center;
}
.gongxi{
  color: #f0ff4b;
  font-size: 47rpx;
}
.getreward{
  color: #feffd0;
  font-size: 33rpx;
}
.layerbtn{
    display: flex;
    justify-content: center;
    align-items: center;
    position: absolute;
    bottom: 10.75%;
    left: 50%;
    margin-left: -33.46%;
    width: 66.92%;
    height: 17.27%;
    color: #080808;
    font-size: 32rpx;
    border-radius: 100rpx;
    box-shadow: 0 10rpx 10rpx rgba(182,0,18,.5);
    background: -webkit-linear-gradient(top,#f5f955,#fdd516);
}
.layerbtn image{
  display: block;
  width: 16.47%;
  height: 46%;
}
.layerbtn:active{
    color: #3a3939;
}
.shaclose{
  z-index: 4;
  position: absolute;
  top: 18.5%;
  right: 0;
  width: 80rpx;
  height: 80rpx;
}
.shaclose image{
  display: block;
  width: 100%;
  height: 100%;
}
