/* pages/personCenter/withdrawProgress/withdrawProgress.wxss */
body{
	background: #fff;
}
.jindutop{
	display: flex;
	align-items: center;
	height: 300rpx;
	padding-left: 30rpx;
	color: #fff;
	font-size: 24rpx;
}
.tixiannum{
	margin: 30rpx 0;
	font-size: 41rpx;
}
.tixiannum text{
	font-size: 67rpx;
}
.ordernum{
	padding: 0 30rpx;
	height: 90rpx;
	line-height: 90rpx;
	font-size: 24rpx;
	color: #444;
	background: #fff;
}
.ordernum text{
	margin-left: 26rpx;
	color: #959595;
}
.mar{
	height: 20rpx;
	background: #f1f3f6;
}
.callservice{
	padding: 0 33rpx;
	height: 100rpx;
	line-height: 100rpx;
	font-size: 26rpx;
	color: #444;
	border-bottom: #f1f3f6 1rpx solid;
}
.callservice text{
	float: right;
	color: #88db45;
	font-size: 70rpx;
}
.buzhou{
	display: flex;
	align-items: center;
	padding: 0 40rpx;
	height: 154rpx;
	color: #959595;
	overflow: hidden;
}
.buzhouleft{
	float: left;
	width: 48rpx;
	margin-right: 42rpx;
}
.buzhouline{
	margin-left: 22rpx;
	width: 5rpx;
	height: 53rpx;
	background: #c7c7c7;
}
.buzhouright{
	float: left;
}
.stepcircle{
	width: 48rpx;
	height: 48rpx;
	line-height: 48rpx;
	text-align: center;
	font-size: 26rpx;
	color: #fff;
	background: #c7c7c7;
	border-radius: 50%;
}
.buzhouright .tit{
	margin-bottom: 22rpx;
	font-size: 32rpx;
}
.buzhouright .text{
	font-size: 24rpx;
}
.alreadygo .buzhouline,.alreadygo .stepcircle{
	background: #36a3ff;
}
.alreadygo .tit,.alreadygo .tit{
	color: #444;
}
.buzhou .whrite{
	background: #fff;
}