/* pages/agent/productChoose/productChoose.wxss */
.header{
    position: relative;
}
.headerBg{
    position: absolute;
    width: 100%;
    left: 0;
    top: 0;
}
.logo{
    position: absolute;
    width: 250rpx;
    top: 186rpx;
    left: 40rpx;
}
.welcome{
    position: absolute;
    top: 236rpx;
    left: 40rpx;
    font-size:32rpx;
    font-weight:bold;
    color:rgba(255,255,255,1);
}

.itemBox{
    position: fixed;
    width: 100%;
    top: 365rpx;
    left: 0;
    padding: 60rpx 40rpx 0; 
    border-radius:45rpx;
    z-index: 10;
    background: white;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
    flex-wrap: wrap;
    text-align: center;
}
.item{
    width: 200rpx;
    height: 200rpx;
    padding: 35rpx 0;
    box-sizing: border-box;
    box-shadow:0px 0px 8px rgba(0,0,0,0.07);
    border-radius:24px;
    margin-bottom: 35rpx;
}
.itemImg{
    width: 80rpx;
    height: 80rpx;
}
.itemTxt{
    font-size:28rpx;
}
.back{
    position: absolute;
    width: 18rpx;
    height: 35rpx;
    top: 71rpx;
    left: 30rpx;
    z-index: 10;
}