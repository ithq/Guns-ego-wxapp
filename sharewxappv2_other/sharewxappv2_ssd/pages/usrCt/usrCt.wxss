page {
  height: 100%;
  background-color: #F5F5F5;
}
.userHeaderCls{
  width: 100%;
  margin-left: 10px;
  margin-top: 10px;
  margin-right: 10px;
  margin-bottom: 10px;
}
.userCenterTitleCls{
  width:95%;
  height:120px;
  background:rgba(255,255,255,1);
  border-radius: 5px;
}
.avatarBoxCls {
  height: 110px;
  margin-left: 15px;
  display: flex;
  align-items: center;
  justify-content:right;
}
.psnInfo{
  position: relative;
  font-size: 18px;
  margin-left: 35rpx;
  border-radius: 5px;
}
.avatarCls{
  margin-left: 20px;
}
.nicknameclass{
  font-size:15px;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:24px;
}
.accountclass{
  font-size:15px;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:24px;
}


.usrCtItems{
  height: 100%;
  width: 100%;
}
.itemBtnCls{
  padding: 0 34rpx;
  width: 100%;
  height: 110rpx;
  border-bottom: 1px solid #f1f3f6;
  line-height: 110rpx;
  background: #fff;
  color: #444;
  font-size: 28rpx;
  text-align: left;
  clear: both;
}
.itemBtnClsLeft{
  float: left;
  width: 50%;
}
.itemBtnClsRight{
  text-align: right;
  margin-right: 30px;
}