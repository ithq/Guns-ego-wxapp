page {
  height: 100%;
  background-color: #F5F5F5;
}

.bgimg {
  background-color: rgb(255, 255, 255);
  text-align: center;
}

.bgimg image {
  width: 750rpx;
  height: 370rpx;
}

.continueTitleBarCls {
  width: 100%;
  height: 160px;
  background: rgba(255, 255, 255, 1);
}

.orderInfoCls {
  width: 100%;
}

.orderInfoTitleCls {
  height: 45px;
  line-height: 45px;
  padding-left: 10px;
  padding: 20rpx 30rpx 0;
  font-size: 32rpx;
  color: #323232;
  background: #fff;
  display: flex;
  align-items: center;
}

.orderInfoItemCls {
  width: 100%;
  height: 48px;
  line-height: 48px;
  font-size: 15px;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(149, 149, 149, 1);
  background-color: #fff;
  padding-left: 25px;
}

.orderInfoItemCls text {
  float: right;
  margin-right: 50px;
  color: #454545;
}

.btns {
  width: 100%;
  border-radius: 5px;
  display: flex;
  align-items: center;
  vertical-align: center;
  justify-content: center;
  margin-bottom: 5rpx;
  margin-top: 20px;
}

.btns form {
  display: block;
  width: 90%;
  height: 100%;
}

.button {
  width: 90%;
}

.button_Up {
  border-radius: 5rpx 5rpx 5rpx 5rpx;
  background-color: rgb(214, 209, 209);
  color: #444;
}

.button_Down {
  background: #000000;
  border-radius: 5rpx 5rpx 5rpx 5rpx;
  color: #fff;
}

.button::after {
  border: 0;
}

.ad-banner {
  position: fixed;
  bottom: 0px;
  width: 100%;
}